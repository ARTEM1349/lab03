#include <stdio.h>

int main()
{ 
   int a, b, i = 2, max = 2;
   scanf("%f",&a);
   scanf("%f",&b);
   while(a > b)
   {
       b++;
       i = 2;
       while(b%i != 0) 
       {
           i++;
       }
       if(b == i && b > max)
       {
          max = b;      
       }      
   }
      return 0;
}



#include <stdio.h>

int main()
{
   
   int a, b, i, max = 2;
   scanf("%f",&a);
   scanf("%f",&b);
   
   do
   {
       b++;
       i = 2;
       do
       {
          i++;
          if(b == i && b > max)
          {
          max = b;      
          }
       }
       while(b%i != 0);  
   }
   while(a >= b); 
 return 0;
